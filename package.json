  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode development",
    "preview": "vite preview",
    "preview:prod": "npm run build && npm run preview",
    "dev:prod": "npm run build && npm run preview -- --port 3000",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "NODE_OPTIONS='--max-old-space-size=8192' vitest run",
    "test:ui": "NODE_OPTIONS='--max-old-space-size=8192' vitest --ui",
    "test:run:full": "NODE_OPTIONS='--max-old-space-size=8192' vitest run",
    "test:coverage": "NODE_OPTIONS='--max-old-space-size=8192' vitest run --coverage",
    "test:watch": "NODE_OPTIONS='--max-old-space-size=8192' vitest",
    "test:ci": "NODE_OPTIONS='--max-old-space-size=8192' vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:install": "playwright install",
    "test:e2e:report": "playwright show-report",
    "test:unit": "NODE_OPTIONS='--max-old-space-size=8192' vitest run --reporter=verbose",
    "test:minimal": "NODE_OPTIONS='--max-old-space-size=4096' vitest run --config vitest.config.minimal.ts",
    "test:basic": "NODE_OPTIONS='--max-old-space-size=2048' vitest run --config vitest.config.basic.ts",
    "test:working": "NODE_OPTIONS='--max-old-space-size=1024' vitest run --config vitest.config.working.ts",
    "test:fixed": "NODE_OPTIONS='--max-old-space-size=2048' vitest run --config vitest.config.fixed.ts",
    "test:imports": "NODE_OPTIONS='--max-old-space-size=2048' vitest run src/__tests__/imports.test.ts",
    "test:unit:watch": "NODE_OPTIONS='--max-old-space-size=8192' vitest --reporter=verbose",
    "test:unit:coverage": "NODE_OPTIONS='--max-old-space-size=8192' vitest run --coverage --reporter=verbose",
    "test:accessibility": "NODE_OPTIONS='--max-old-space-size=8192' vitest run --reporter=verbose",
    "test:auth": "NODE_OPTIONS='--max-old-space-size=8192' vitest run --reporter=verbose",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:services": "playwright test comprehensive-services-e2e.spec.ts",
    "test:services:headed": "playwright test comprehensive-services-e2e.spec.ts --headed",
    "test:quick": "node scripts/quick-test.cjs",
    "test:manual": "node scripts/manual-testing-checklist.cjs",
    "test:lighthouse": "node scripts/lighthouse-performance.cjs",
    "test:performance": "node scripts/performance-check.cjs",
    "test:cross-browser": "node scripts/cross-browser-test.cjs",
    "test:mobile": "node scripts/mobile-test.cjs",
    "test:comprehensive": "npm run test:manual && npm run test:performance && npm run test:cross-browser && npm run test:mobile",
    "test:docker": "node scripts/docker-test-runner.js",
    "test:run": "node scripts/run-tests.js",
    "test:verify": "node scripts/test-verification.js",
    "test:docker:unit": "docker compose -f docker-compose.test.yml run --rm test-runner npm run test:unit:coverage",
    "test:docker:e2e": "docker compose -f docker-compose.test.yml run --rm test-runner npm run test:e2e:headless",
    "test:docker:build": "docker compose -f docker-compose.test.yml build --no-cache",
    "test:docker:clean": "docker compose -f docker-compose.test.yml down -v --remove-orphans",
    "analyze": "node analyze-bundle.js",
    "build:analyze": "npm run build && npm run analyze",
    "security:review": "node scripts/security-review.cjs"
  },
