# API Configuration
VITE_API_BASE_URL=https://api.example.com
VITE_API_KEY=your_api_key_here

# Authentication
VITE_AUTH_DOMAIN=your-auth-domain.auth0.com
VITE_AUTH_CLIENT_ID=your-auth0-client-id
VITE_AUTH_AUDIENCE=your-api-audience
VITE_AUTH_REDIRECT_URI=http://localhost:3000/callback

# Environment (development, production, test)
NODE_ENV=development

# Feature Flags
VITE_FEATURE_NEW_SIGNUP=true
VITE_FEATURE_ANALYTICS=false

# Logging
VITE_LOG_LEVEL=debug
VITE_ENABLE_DEV_TOOLS=true

# Webhook Secrets (for development only - use secure secret management in production)
NEXT_PUBLIC_WEBHOOK_SECRET_1=your_webhook_secret_1_here
NEXT_PUBLIC_WEBHOOK_SECRET_2=your_webhook_secret_2_here

# External Services
VITE_GOOGLE_ANALYTICS_ID=UA-XXXXX-Y
VITE_SENTRY_DSN=your-sentry-dsn
