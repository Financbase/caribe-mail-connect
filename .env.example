# Vite environment variables for Supabase and app config
# Copy this file to .env or .env.local and fill in your values

# Demo mode (bypasses real Supabase auth in dev if supported by the app)
VITE_USE_DEMO_MODE=false

# Supabase (Frontend)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# Application
VITE_APP_ENV=development
VITE_APP_VERSION=0.0.0
NODE_ENV=development

# API
VITE_API_BASE_URL=http://localhost:3000

# Logging / DevTools
VITE_LOG_LEVEL=info
VITE_ENABLE_DEV_TOOLS=false
